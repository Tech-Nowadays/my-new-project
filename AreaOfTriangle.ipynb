   "source": [
    "import math\n",
    "a = float(input(5.5))\n",
    "b = float(input(6))\n",
    "c = float(input(7))\n",
    "\n",
    "s = (a+b+c)/2 #chu vi tam giác\n",
    "area = math.sqrt(s*(s-a)*(s-b)*(s-c)) #area: diện tích hình tam giác #math.sqrt: hàm tính căn bậc 2\n",
    "\n",
    "print(\"area of triangle is: \", area)\n",
    "print(type(area))"
   ]
